    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical" >

    <Button
        android:id="@+id/audio"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/button2"
        android:layout_margin="10dp"
        android:text="Audio" />

    <Button
        android:id="@+id/cal"

        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/button2"
        android:layout_margin="10dp"
        android:text="Calclator Sum" />

    <Button
        android:id="@+id/web"
        android:background="@drawable/button2"
        android:layout_margin="10dp"

        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Web View" />

    <Button
        android:id="@+id/asset"
        android:background="@drawable/button2"
        android:layout_margin="10dp"

        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Web View Asset 7kyat" />

    <Button
        android:id="@+id/alert"
        android:background="@drawable/button2"
        android:layout_margin="10dp"

        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Alert" />
    <Button
        android:id="@+id/shard"
        android:background="@drawable/button2"
        android:layout_margin="10dp"

        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Shard Prafrance" />

    <Button
        android:id="@+id/video"
        android:background="@drawable/button2"
        android:layout_margin="10dp"

        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Vedio" />

    <Button
        android:id="@+id/color_picker"
        android:background="@drawable/button2"
        android:layout_margin="10dp"

        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/dialog_color_picker" />

    <Button
        android:id="@+id/gallry"
        android:background="@drawable/button2"
        android:layout_margin="10dp"

        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Gallry" />
